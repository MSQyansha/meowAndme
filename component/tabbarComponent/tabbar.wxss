.tabbar_box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 98rpx;
  box-shadow: 0 -1px 4rpx rgba(240, 241, 243,1);
}

.tabbar_box.iphoneX-height {
  padding-bottom: 66rpx;
}

.middle-wrapper {
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}

.middle-wrapper.iphoneX-height {
  bottom: 66rpx;
}

.tabbar_nav {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  height: 100%;
  position: relative;
  
}

.tabbar_icon {
 width:46rpx;
height:46rpx;

}

.tabbar_icon_middle {
  width: 84rpx;
  height: 84rpx;
}

.special-wrapper {
  position: absolute;
  /* left: 77rpx; */
  top: -36rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  text-align: center;
  box-sizing: border-box;
  border-top: 2rpx solid #F0F1F3;
  background-color: #fff;
  padding: 4rpx;
}

.special-wrapper .tabbar_icon {
  width: 84rpx;
  height: 84rpx;
}

.special-text-wrapper {
  width: 56rpx;
  height: 48rpx;
}